! █▓▒░ fonts
urxvt*font: xft:Inconsolata:size=11:antialias=true
urxvt*boldFont: xft:Inconsolata:size=11:antialias=true

! █▓▒░ interface
urxvt*scrollBar: false
!urxvt*transparent: true
!urxvt*shading: 15

! █▓▒░ mikado colors
urxvt*foreground: #f3f3f3
urxvt*depth: 32
urxvt*background: [75]#121212
urxvt*cursorColor: #f3f3f3
urxvt*color0: #322a2c
urxvt*color8: #46423b
urxvt*color1: #a04363
urxvt*color9: #ae837a
urxvt*color2: #9b9329
urxvt*color10: #b4aa30
urxvt*color3: #bf7a29
urxvt*color11: #c7a551
urxvt*color4: #6a8c8c
urxvt*color12: #74999e
urxvt*color5: #856774
urxvt*color13: #9c818e
urxvt*color6: #757978
urxvt*color14: #9fa590
urxvt*color7: #bcbcaf
urxvt*color15: #c1c4bc
urxvt*color0: #111111
urxvt*color8: #666666


! █▓▒░ misc
urxvt.saveLines: 4095
urxvt.iso14755: false
!URxvt*altSendsEscape: true

Urxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

Xft.hinting: true 
Xft.hintstyle: hintfull 
Xft.antialias: rgba 
Xft.rgba: rgb

!URxvt.perl-ext-common: font-size
!URxvt.keysym.C-+:     perl:font-size:increase
!URxvt.keysym.C--:   perl:font-size:decrease
!URxvt.keysym.C-S-Up:   perl:font-size:incglobal
!URxvt.keysym.C-S-Down: perl:font-size:decglobal
